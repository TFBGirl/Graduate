/* miniprogram/pages/my/my.wxss */
.information{
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #ad68f3;
  color: white;
  height: 160px;
}
.avatar{
  height: 90px;
  width: 90px;
  border-radius: 50px;
  margin: 20px 0px 10px 0px;
  overflow: hidden;
}
.my{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-bottom: 10px;
}
.CarsNum{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20px;
  font-weight: bold;
  margin-left: 20px;
}
.goodsNum{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20px;
  font-weight: bold;
  margin-right: 20px;
}
.set{
  width: 100%;
}
.set navigator{
  width: 100%;
  height: 60rpx;
  padding-left: 20rpx;
  display: flex;
  align-items: center;
  
}